.header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 300px;

    &__logo {
      width: 10%;
    }

    &__menu {
      width: 100%;
      @include tablets {
      display: none;
      }    
    }

    &__socials {
      width: 10%;
      @include phones {
      display: none;
     }
       @include tablets {
       width: 20%;
      }
    }
  }
  