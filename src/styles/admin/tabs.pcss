.tabs {
    background: #fff;
    }

.tabs__list {
    display: flex; 
    list-style: none;  
    color:#60697c; 
    padding: 0;
    }

.tabs__item {
    border-bottom: 2px solid transparent; 
    padding: 30px;
}

.tabs__item-active {
    border-bottom: 2px solid #383ace; 
    color: #383ace;
}

.tabs__link {
    text-decoration:none; 
    font-weight:600;
}

.container-tabs {
    padding:10px;
}